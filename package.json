    "gemini-coverage": "~0.3.0",
    "handlebars": "^4.0.5",
    "inherit": "~2.2.1",
    "install": "^0.6.1",
    "js-yaml": "^3.2.5",
    "lodash": "^3.10.1",
    "looks-same": "^3.0.0",
