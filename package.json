    "test-browser": "istanbul test _mocha test/browser",
    "test": "istanbul test _mocha -- --recursive test/unit test/functional test/browser",
    "lint": "eslint .",
    "precommit": "npm run lint",
    "preversion": "npm run lint && npm test"
