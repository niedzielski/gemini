    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gemini-testing/gemini.git"
  },
  "keywords": [
    "test",
    "testing",
    "screenshot",
    "selenium",
    "layout"
  ],
  "bugs": {
