  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "coveralls": "^2.11.2",
    "husky": "^0.11.4",
    "istanbul": "^0.4.2",
    "eslint": "^3.0.1",
    "mocha": "^2.1.0",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.3",
    "uglify-js": "2.6.4",
    "eslint-config-gemini-testing": "^1.0.4"
  },
  "scripts": {
    "test-unit": "istanbul test _mocha -- --recursive test/unit",
